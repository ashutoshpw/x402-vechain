{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [
          "dist/**",       
          ".vercel/**",    
          ".astro/**",
          "worker.js" ,
          "build/**"     
        ]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "test": {
        "outputs": ["coverage/**"]
      },
      "test:coverage": {
        "outputs": ["coverage/**"]
      },
      "lint": {
        "outputs": []
      },
      "check-types": {
        "outputs": []
      }
    }
  }